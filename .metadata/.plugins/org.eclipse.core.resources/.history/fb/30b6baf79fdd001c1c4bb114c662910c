package com.Question4;

public class Main {

	public static void main(String[] args) {
		
		Animal[] animals = new Animal[3];
		
		animals[0] = new Dog();
		animals[1] = new Cat();
		animals[2] = new Tiger();
		
		System.out.println("Dog all methods");
		
		animals[0].eat();
		animals[0].makeNoise();
		animals[0].walk();
		
	}
}
